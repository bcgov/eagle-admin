
    <div nz-flex nzVertical nzGap="small">
      <nz-switch [(ngModel)]="open" nzCheckedChildren="open" nzUnCheckedChildren="close"></nz-switch>
      <nz-cascader
        [nzOptions]="nzOptions"
        [ngModel]="values"
        [nzOpen]="open"
        (nzSelectionChange)="onSelectionChange($event)"
        (nzVisibleChange)="onVisibleChange($event)"
      />
    </div>
  